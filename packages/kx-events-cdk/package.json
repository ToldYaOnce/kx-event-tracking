{
  "name": "@toldyaonce/kx-events-cdk",
  "version": "1.0.22",
  "description": "CDK constructs and infrastructure for event tracking system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "kx-events-cdk": "dist/bin/stack.js"
  },
  "files": [
    "dist/**/*",
    "src/lambdas/**/*"
  ],
  "scripts": {
    "build": "npm run prepare-consumer && tsc",
    "clean": "rm -rf dist",
    "test": "echo \"No tests yet\"",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "synth": "cdk synth",
    "prepare-consumer": "node scripts/prepare-lambda.js && cd src/lambdas/events-consumer && npx tsc index.ts db.ts --target es2020 --module commonjs --esModuleInterop --skipLibCheck"
  },
  "dependencies": {
    "aws-cdk-lib": "2.101.1",
    "constructs": "^10.0.0",
    "@aws-sdk/client-rds": "^3.0.0",
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/pg": "^8.10.0",
    "@types/aws-lambda": "^8.10.0",
    "typescript": "^5.0.0",
    "aws-cdk": "2.101.1",
    "esbuild": "^0.19.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toldyaonce/kx-event-tracking.git"
  }
}
